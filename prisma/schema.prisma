generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  sessions Sessions[]
  @@map("users")
}
model Sessions {
  id        Int      @id @default(autoincrement())
  user    Users    @relation(fields: [userId], references: [id])
  userId    Int
  token     String
  createdAt DateTime @default(now())
  @@map("sessions")
}